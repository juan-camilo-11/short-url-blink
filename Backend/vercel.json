{
    "builds": [
        {
            "src": "build/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "build/app.js",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        },
        {
            "src": "/url/(.*)",
            "dest": "build/app.js",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        },
        {
            "src": "/profile/(.*)",
            "dest": "build/app.js",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}